generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Works {
  id          String   @id @default(autoincrement())
  image       String
  title       String
  description String
  link        String
}

model TechStack {
  id          String   @id @default(autoincrement())
  name        String
  image       String
}